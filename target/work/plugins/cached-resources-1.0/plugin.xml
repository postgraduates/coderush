<plugin name='cached-resources' version='1.0' grailsVersion='1.2 &gt; *'>
  <author>Marc Palmer</author>
  <authorEmail>marc@grailsrocks.com</authorEmail>
  <title>Cached Resources</title>
  <description>\
Makes static resources browser-cacheable with unique filenames based on their content
</description>
  <documentation>http://grails.org/plugin/cached-resources</documentation>
  <resources>
    <resource>BootStrap</resource>
    <resource>BuildConfig</resource>
    <resource>Config</resource>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>org.grails.plugin.cachedresources.HashAndCacheResourceMapper</resource>
  </resources>
  <dependencies>
    <plugin name='resources' version='1.0.RC3 &gt; *' />
    <plugin name='cacheHeaders' version='1.0.4 &gt; *' />
  </dependencies>
  <behavior />
</plugin>